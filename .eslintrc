{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",

    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": 0
    },
    "extends": ["airbnb", "plugin:prettier/recommended", "plugin:jest/recommended"]
}