        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",

    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    },
    "extends": ["plugin:prettier/recommended"]
}